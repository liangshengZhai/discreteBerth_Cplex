{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug CPLEX Program",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/main",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "DYLD_LIBRARY_PATH",
                    "value": "/Applications/CPLEX_Studio2211/cplex/lib/arm64_osx/static_pic:/Applications/CPLEX_Studio2211/concert/lib/arm64_osx/static_pic"
                }
            ],
            "externalConsole": false,
            "MIMode": "lldb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}